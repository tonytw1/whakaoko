{
  "title": "Whakaoko",
  "description": "A service for aggregating 3rd party content from a variety of sources (RSS / Twitter / Instagram) into an easy to consume local JSON or RSS feed.",
  "version": "1.0.0",
  "components": {
    "schemas": {
      "Channel": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "username": {
            "type": "string"
          }
        }
      },
      "FeedItem": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "url": {
            "type": "string"
          },
          "body": {
            "type": "string"
          },
          "subscriptionId": {
            "type": "string"
          },
          "channelId": {
            "type": "string"
          }
        }
      },
      "Subscription": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "url": {
            "type": "string"
          },
          "username": {
            "type": "string"
          },
          "latestItemDate": {
            "type": "string"
          },
          "lastRead": {
            "type": "string"
          },
          "etag": {
            "type": "string"
          },
          "classification": {
            "type": "string"
          }
        }
      }
    }
  }
}
